{
  "manifest_version": 2,
  "name": "Skype Bot",
  "version": "0.3",
  "content_scripts": [
	  {
	    "matches": [
	      "<all_urls>"
	    ],
	    "js": ["plugins/jquery-2.1.4.min.js", "plugins/cleverbot.io.min.js", "plugins/socket.io.js", "plugins/sendkeys.js", "plugins/bilrange.js", "lib/conversations.js", "lib/messages.js", "lib/chat.js", "lib/util.js", "config.js", "lib/commands.js", "lib/console_commands.js", "content.js" ]
	  }
	],
	"browser_action": {
  		"default_icon": "icon.png"
	},
	"background": {
  		"scripts": ["background.js"]
	}
}